//*********************************************************************
//* THIS JOB DELETES AND RE-CREATES A QSAM DATASET 
//*********************************************************************
//IBMUSER3 JOB ,'IBMUSER',CLASS=A,
//         MSGCLASS=X,
//         MSGLEVEL=(1,1),
//         NOTIFY=&SYSUID
//DELETE   EXEC PGM=IEFBR14
//SYSPRINT DD SYSOUT=*
//SYSOUT   DD SYSOUT=*
//SYSDUMP  DD SYSOUT=*
//DD1      DD DSN=IBMUSER.QSAM1,
//            DISP=(MOD,DELETE,DELETE),
//            SPACE=(TRK,(0,0))
//CREATE   EXEC PGM=IEFBR14
//SYSPRINT DD SYSOUT=*
//SYSOUT   DD SYSOUT=*
//SYSDUMP  DD SYSOUT=*
//DD1      DD DSN=IBMUSER.QSAM1,
//            DISP=(NEW,CATLG,DELETE),
//            SPACE=(TRK,(1,1),RLSE),
//            UNIT=SYSDA,
//            DCB=(DSORG=PS,RECFM=FB,LRECL=80,BLKSIZE=16000)